{ 
    "babelrcRoots": [
        "packages/core",
        "packages/emisor",
        "packages/plugins/*"
    ],
    "presets": [
        ["@babel/preset-env", {
            "modules": false
        }]
    ],
    "plugins": [
        "@babel/plugin-proposal-private-methods",
        "@babel/plugin-proposal-class-properties"
    ],
    "env": {
        "test": {
            "presets": [
                ["@babel/preset-env", {
                    "targets": {
                        "node": "current"
                    }
                }]
            ]
        }
    }
}