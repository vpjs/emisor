{ 
    "babelrcRoots": [
        "packages/core",
        "packages/emisor",
        "packages/plugins/*"
    ],
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "esmodules": true
            }
        }]
    ],
    "plugins": [
        "@babel/plugin-proposal-private-methods",
        "@babel/plugin-proposal-class-properties"
    ]
}